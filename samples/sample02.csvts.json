{
  "$schema": "https://schemahub.openpotato.org/csv-table-schema/v0.1/schema.json",
  "version": "0.3",
  "title": "CSV Table Schema for configuration",
  "description": "A sample CSV Table Schema for configuration data",
  "dictionary": {
    "name": "Configuration.csv",
    "language": "en",
    "keys": [
      {
        "id": "timeZone",
        "name": "TimeZone",
        "description": "Time Zone according to http://web.cs.ucla.edu/~eggert/tz/tz-link.htm",
        "type": "string"
      },
      {
        "id": "validFrom",
        "name": "ValidFrom",
        "description": "Start date of validity",
        "type": "date",
        "formats": [
          "yyyy-MM-dd"
        ]
      },
      {
        "id": "validTo",
        "name": "ValidTo",
        "description": "End date of validity",
        "type": "date",
        "formats": [
          "yyyy-MM-dd"
        ]
      }
    ]
  }
}